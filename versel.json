{
    "version": 2,
    "rewrites" : [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }